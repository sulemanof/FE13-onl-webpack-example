{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "plugins": ["react"],
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module",
        
    },
    "rules": {
        "import/prefer-default-export": "off"
    },
    "overrides": [
        {
            "files": ["webpack.*.js"],
            "rules": {
                "import/no-extraneous-dependencies": "off"
            }
        }
    ]
}
